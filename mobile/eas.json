{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "http://192.168.1.2:5000"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_BASE_URL": "http://192.168.1.2:5000"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_BASE_URL": "http://192.168.1.2:5000"
      }
    }
  },
  "submit": {
    "production": {}
  }
}